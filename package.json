{
  "name": "fascinante-digital",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm --filter app dev\" \"pnpm --filter web-publica dev\"",
    "build": "pnpm --filter app build",
    "lint": "pnpm --filter app lint",
    "dev:app": "pnpm --filter app dev",
    "dev:web": "pnpm --filter web-publica dev",
    "build:all": "pnpm -r build",
    "lint:all": "pnpm -r lint"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
